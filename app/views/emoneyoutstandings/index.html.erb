<% @emoneyoutstandings.each do |emoneyoutstanding| %>
  <tr>
    <td><%= emoneyoutstanding.amount %></td>
    <td><%= emoneyoutstanding.date %></td>
    <td><%= link_to 'Show', emoneyoutstanding %></td>
