<%= form_for(@address) do |f| %>
  <%= render "shared/error_messages", :target => @address %>

  <div class="field">
    <%= f.label :party_id %><br />
    <%= f.text_field :party_id, :disabled => true %>
  </div>
  <div class="field">
    <%= f.label :street %><br />
    <%= f.text_field :street %>
  </div>
  <div class="field">
    <%= f.label :housenumber %><br />
    <%= f.text_field :housenumber %>
  </div>
  <div class="field">
    <%= f.label :postal_supplement %><br />
    <%= f.text_field :postal_supplement %>
  </div>
  <div class="field">
    <%= f.label :zipcode %><br />
    <%= f.text_field :zipcode %>
  </div>
  <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </div>
  <div class="field">
    <%= f.label :country_id %><br />
    <%= f.text_field :country_id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
